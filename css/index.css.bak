/********模态登录框*****/
.modal,.moda2{
	position:fixed;  top:0;  right:0;
	bottom:0;  left:0; display: none;
	background:rgba(0,0,0,0.5);
}
.modal-dialog,.moda2-dialog{width:400px;margin:250px auto;}
.modal-content,.moda2-content{
	background:#fff;
	border-radius:4px;
	box-shadow:0 0 10px #000;
	padding:10px;
}
.moda1 h3,.moda2 h3{font-size:1.5em;margin:10px 0 15px;}
.modal .hint,.moda2 .hint{
	border:1px solid #e4393c;
	padding:4px;
	background:#fecfdf;
}
.modal input,.moda2 input{
	box-sizing:border-box;/*总宽=间距+宽*/
	display:block;
	width:100%;
	padding:6px 10px;
	margin:20px 0;
	border:1px solid #aaa;
}
.modal input[type="button"],.moda2 input[type="button"]{
	background:#e4393c;
	font-size:1.5em;
	cursor:pointer;
	color:#fff;
}
/***开场动画**/
.animation{
	position:fixed; top:0; right:0; bottom:0; left:0;
	background:rgba(0,0,0,0.5);
}
.animation-dialog{width:900px;margin:250px auto;}
.animation-content{
	background:transparent;
	padding:10px; position:relative;
}
.animation-content a{
	border-radius:15px;
	position:absolute; top:0; right:0;
	display:inline-block; width:30px; height:30px;
	color:#fff; font-size:2em;
}